proc at
    int ptr
    int i
    i ! ptr !
    ptr @ i @ 16 * -
end

proc inc
    int ptr
    ptr !
    ptr @ @ 1 + ptr @ !
end

proc print_array
    int ptr
    int length
    int i
    length ! ptr ! 0 i !
    while i @ length @ < do
        ptr @ i @ at @ .
        i inc
    end
end

proc main
    int arr_length
    5 arr_length !
    arr 5 int a

    int i 0 i !
    while i @ arr_length @ < do
        i @ 2 * a i @ at !
        i inc
    end

    a arr_length @ print_array

end